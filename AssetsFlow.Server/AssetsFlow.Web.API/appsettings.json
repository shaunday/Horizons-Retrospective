{
  "PaginationSettings": {
    "DefaultPageSize": 10,
    "MaxPageSize": 20
  },
  "CacheSettings": {
    "CacheDurationMinutes": 5,
    "PreloadPageCount": 3,
    "SizeLimit": 1024,
    "MaxConcurrentUsers": 1000,
    "LoadWaitTimeoutSeconds": 2,
    "CleanupInactiveUsersThresholdHours": 2,
    "CleanupIntervalMinutes": 30
  },
  "UserServiceClient": {
    "TimeoutSeconds": 30,
    "MaxRetryAttempts": 3,
    "EnableRetryOnFailure": true
  },
  "Jwt": {
    "SecretKey": "${JWT_SECRET_KEY}",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}",
    "ExpirationMinutes": "${JWT_EXPIRATION_MINUTES}"
  }
}