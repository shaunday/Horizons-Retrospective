{
  "type": "module",
  "name": "trading-chattie-backend",
  "version": "0.9.401",
  "description": "Validate 1 line trade creations with AI",
  "main": "dist/app.js",
  "imports": {
    "#services/*": "./dist/services/*.js",
    "#routes/*": "./dist/routes/*.js"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsc --project tsconfig.dev.json --watch",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "build": "tsc --project tsconfig.build.json",
    "start": "cross-env NODE_ENV=development node dist/app.js",
    "start:prod": "cross-env NODE_ENV=production node dist/app.js",
    "lint": "eslint src --ext .ts --fix",
    "audit": "npm audit --audit-level=moderate"
  },
  "author": "Shaun Day",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^5.1.0",
    "pino": "^9.7.0",
    "pino-http": "^8.6.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.21.3",
    "pino-pretty": "^13.0.0",
    "typescript": "^5.9.2"
  }
}