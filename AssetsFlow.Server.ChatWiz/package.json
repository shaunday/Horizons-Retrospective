{
  "type": "module",
  "name": "trading-chattie-backend",
  "version": "0.9.401",
  "description": "Validate 1 line trade creations with AI",
  "main": "src/app.mjs",
  "imports": {
    "#services/*": "./src/services/*",
    "#routes/*": "./src/routes/*"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch src --ext js,mjs,json src/app.mjs",
    "start": "cross-env NODE_ENV=production node src/app.mjs",
    "build": "npm run start",
    "lint": "eslint src --fix",
    "audit": "npm audit --audit-level=moderate"
  },
  "author": "Shaun Day",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "pino": "^9.7.0",
    "pino-http": "^8.6.1"
  },
  "devDependencies": {
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.21.3",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.0.0"
  }
}