      <PackageReference Include="Serilog" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\MarketWatch.All\MW.Price.Fetcher.Adapters\MW.Price.Fetcher.Adapters.csproj" />
    </ItemGroup>

</Project>
